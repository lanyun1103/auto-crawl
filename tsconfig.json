{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": true,
    "module": "es6",
    "target": "es5",
    "allowJs": true
  },
  "paths": {
    "@/*": [
      "core/src/*"
    ]
  },
  "include": ["core/src/*"],    /* 需要编译的文件或目录 */
  "exclude": ["node_modules"] /* 排除不需要编译的文件或目录 */
}
